{
  "canisters": {
    "blog": {
      "main": "canisters/blog/main.mo",
      "type": "motoko"
    },
      "gallery": {
      "main": "canisters/blog/gallery.mo",
      "type": "motoko"
    },
    "file_scaling_manager": {
      "main": "frontend/src/FileScalingManager.mo",
      "type": "motoko",
      "dependencies": []
    },
    "file_storage": {
      "main": "frontend/src/FileStorage.mo",
      "type": "motoko",
      "dependencies": []
    },
    "assets": {
      "dependencies": [
        "blog",
        "gallery"
      ],
